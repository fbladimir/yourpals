generator client {
  provider      = "prisma-client-js"
  output        = "../prisma-client"
  binaryTargets = ["native", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model TestTable {
  id   Int    @id @default(autoincrement())
  name String
}
